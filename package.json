{
  "name": "gmat-quant-snake",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://KH7588.github.io/gmat-quant-snake",
  "dependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "lucide-react": "^0.300.0"
  },
  "devDependencies": {
    "esbuild": "^0.23.0",
    "gh-pages": "^6.1.1"
  },
  "scripts": {
    "start": "node copy-assets.js && esbuild index.tsx --bundle --format=esm --servedir=dist --outfile=dist/index.js --watch --loader:.tsx=tsx --loader:.ts=ts",
    "build": "node clean.js && node copy-assets.js && esbuild index.tsx --bundle --splitting --format=esm --minify --sourcemap --outdir=dist --target=es2020 --loader:.tsx=tsx --loader:.ts=ts",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}